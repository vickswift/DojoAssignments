<div class="container">

  <h1>List Users</h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <td>First Name</td>
        <td>Last Name</td>
        <td>Created At</td>
      </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.created_at %></td>
      </tr>
    <% end %>
    </tbody>
  </table>

  <a href="/users/new"><button type="button" class="btn btn-default">Add New User</button></a>

</div>
